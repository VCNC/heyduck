{
  "name": "heyduck",
  "version": "1.0.0",
  "repository": "github.com/VCNC/heyduck",
  "scripts": {
    "start": "NODE_ENV=production ts-node ./src/server.ts",
    "dev": "NODE_ENV=development nodemon --ignore '/test/*.ts' --watch '*.ts' --exec 'ts-node' ./src/server.ts",
    "build:front": "esbuild theme/fun.js --bundle --watch --outfile=theme/output/fun.js",
    "mockDB": "ts-node ./test/lib/mockDatabase.ts",
    "test": "NODE_ENV=testing mocha -r ts-node/register test/*-test.ts --exit",
    "lint": "eslint --ext .ts ./src/ || exit 0",
    "lint:fix": "eslint --fix --ext .ts ./src/ || exit 0"
  },
  "license": "MIT",
  "dependencies": {
    "@slack/bolt": "^4.2.1",
    "@slack/web-api": "^6.8.1",
    "air-datepicker": "^3.5.0",
    "bog": "^1.0.2",
    "dotenv": "^16.0.3",
    "mongodb": "^5.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/dotenv": "^6.1.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.1.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "esbuild": "0.20.2",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "mongodb-memory-server": "8.12.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "proxyquire": "^2.1.3",
    "sinon": "^15.0.4",
    "sinon-chai": "^3.7.0"
  }
}
